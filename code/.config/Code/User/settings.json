  "vim.vimrc.enable": true,
  "vim.leader": " ",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"],
      "silent": true
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"],
      "silent": true
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["workbench.action.toggleSidebarVisibility"],
      "silent": true
    },
    {
      "before": ["<leader>", "e", "e"],
      "commands": ["workbench.view.explorer"],
      "silent": true
    },
    {
      "before": ["<leader>", "t", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"],
      "silent": true
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["lazygit-vscode.toggle"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "g"],
      "commands": ["workbench.action.findInFiles"],
      "silent": true
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.gotoSymbol"],
      "silent": true
    },
    {
      "before": ["<leader>", "h", "r"],
      "commands": ["git.revertSelectedRanges"],
      "silent": true
    },
    {
      "before": ["<leader>", "h", "d"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["<leader>", "h", "n"],
      "commands": ["editor.action.dirtydiff.next"],
      "silent": true
    },
    {
      "before": ["<leader>", "h", "p"],
      "commands": ["editor.action.dirtydiff.previous"],
      "silent": true
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["editor.action.trimTrailingWhitespace"],
      "silent": true
    },
    {
      "before": ["/"],
      "commands": ["actions.find"],
      "silent": true
    },
    {
      "before": ["n"],
      "commands": ["editor.action.nextMatchFindAction", "extension.vim_escape"],
      "silent": true
    },
    {
      "before": ["N"],
      "commands": [
        "editor.action.previousMatchFindAction",
        "extension.vim_escape"
      ],
      "silent": true
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["closeFindWidget"],
      "silent": true
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"],
      "silent": true
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["editor.action.marker.next"],
      "silent": true
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["editor.action.formatDocument"],
      "silent": true
    },
    {
      "before": ["<C-w>", "L"],
      "commands": ["workbench.action.moveActiveEditorGroupRight"],
      "silent": true
    },
    {
      "before": ["<C-w>", "H"],
      "commands": ["workbench.action.moveActiveEditorGroupLeft"],
      "silent": true
    },
    {
      "before": ["<C-w>", "J"],
      "commands": ["workbench.action.moveActiveEditorGroupDown"],
      "silent": true
    },
    {
      "before": ["<C-w>", "K"],
      "commands": ["workbench.action.moveActiveEditorGroupUp"],
      "silent": true
    },
    {
      "before": ["<C-w>", "r"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"],
      "silent": true
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["t", "t"],
